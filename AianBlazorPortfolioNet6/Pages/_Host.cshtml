@page "/"
@namespace AianBlazorPortfolioNet6.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>My Blazor App</title>
    <base href="/" />
    <link href="https://fonts.googleapis.com/css2?family=Be+Vietnam+Pro:wght@400;600&display=swap" rel="stylesheet" />
    <link rel="stylesheet" href="css/app.css" />
</head>
<body>
    <app>
        <!-- Render mode set to "Server" disables prerendering -->
        <component type="typeof(App)" render-mode="Server" />
    </app>
    <script src="js/slider.js"></script>
    <script src="js/sectionObserver.js"></script>
    <script src="_framework/blazor.server.js"></script>
</body>
</html>
